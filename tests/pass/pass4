DECLARATION OF one
    CONST ten = 10;
    VAR twelve : thirteen;
    TYPE nine : ARRAY fourteen[0..42] OF hello ;
    PROCEDURE two
    FUNCTION three
DECLARATION END

IMPLEMENTATION OF one

    PROCEDURE two;
        FUNCTION three;
            IF hello + goodbye THEN
                word := cactus
            END IF;
        FOR bottle := apple DO
            hello := goodbye
        END FOR;

    WHILE three DO
        DO
            four := five * six
        WHILE seven END DO
    END WHILE
.
